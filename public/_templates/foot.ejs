<script>
 /**
 * Initiate WOW.js
 */
wow = new WOW(
	{
		boxClass:     'wow',      // default
		animateClass: 'animated', // default
		offset:       0,          // default
		mobile:       false,       // default
		live:         true        // default
	}
)
wow.init();
 
 /**
  * Initiate Syntaxt Highlighting
  */
 hljs.initHighlightingOnLoad();
</script>

<% if( current.source == 'index') { %>
<!-- Load Vivus -->
	<script>
		var myMac = new Vivus('my-mac-svg', {
			type: 'oneByOne',
			duration: 500,
			pathTimingFunction: Vivus.EASE_OUT,
			animTimingFunction: Vivus.EASE_IN
		});
	</script>
<% } %>

